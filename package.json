{
  "name": "onshape-docureader",
  "type": "module",
  "version": "1.0.0",
  "description": "Using OnShape API to gather information about documents",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && vite build",
    "start": "node dist/index.js",
    "dev": "concurrently \"nodemon src/index.ts\" \"vite\" \"npm run open-browser\"",
    "open-browser": "sh -c 'sleep 3 && wslview http://localhost:5173'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run spec",
    "test": "echo \"Error: no test specified\" && exit 1",
    "spec": "python project_tools/generate_spec.py . -o AUTO_SPEC.md --ignore project_tools examples notes",
    "spec:preview": "python project_tools/generate_spec.py . --stdout --ignore project_tools examples notes | head -150"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ulises-c/OnShape-DocuReader.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ulises-c/OnShape-DocuReader/issues"
  },
  "homepage": "https://github.com/ulises-c/OnShape-DocuReader#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "better-sqlite3": "^12.4.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "jszip": "^3.10.1",
    "morgan": "^1.10.1",
    "p-limit": "^7.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.9"
  }
}
